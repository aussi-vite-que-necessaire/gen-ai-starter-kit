# =============================================================================
# GEN AI STARTER KIT - Environment Variables
# =============================================================================
# Copy this file to .env and fill in the values

# --- Server ---
NODE_ENV=development
PORT=3000

# --- URLs ---
FRONTEND_URL=http://localhost:5173
BETTER_AUTH_URL=http://localhost:3000

# --- Database ---
DATABASE_URL=postgresql://app_user:your_password@localhost:5432/webapp
DB_PASSWORD=your_password

# --- Redis ---
REDIS_HOST=localhost
REDIS_PORT=6379

# --- Auth ---
BETTER_AUTH_SECRET=your_secret_key_change_this_in_production

# --- n8n ---
N8N_URL=http://localhost:5678
N8N_ENCRYPTION_KEY=your_n8n_encryption_key
INTERNAL_API_SECRET=your_internal_secret_for_n8n_api_calls
